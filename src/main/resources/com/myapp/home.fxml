<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.myapp.HomeController">
    <VBox alignment="CENTER" spacing="20.0">
        <children>
            <Label text="Home" />
            <Button fx:id="itemizerButton" text="Switch to Itemizer" onAction="#switchToItemizer" />
            <Button fx:id="libraryButton" text="Switch to Library" onAction="#switchToLibrary" />
        </children>
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
        </padding>
    </VBox>
</AnchorPane>
